<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="PanelSwitchLayout">
        <attr name="animationSpeed" format="integer">
            <enum name="slowest" value="300" />
            <enum name="slow" value="250" />
            <enum name="standard" value="200" />
            <enum name="fast" value="150" />
            <enum name="fastest" value="100" />
        </attr>
    </declare-styleable>

    <declare-styleable name="PanelView">
        <attr name="panel_trigger" format="reference" />
        <attr name="panel_toggle" format="boolean" />
        <attr name="panel_layout" format="reference" />
    </declare-styleable>

    <declare-styleable name="LinearContentContainer">
        <attr name="linear_edit_view" format="reference" />
        <attr name="linear_auto_reset_enable" format="boolean" />
        <attr name="linear_auto_reset_area" format="reference" />
    </declare-styleable>

    <declare-styleable name="FrameContentContainer">
        <attr name="frame_edit_view" format="reference" />
        <attr name="frame_auto_reset_enable" format="boolean" />
        <attr name="frame_auto_reset_area" format="reference" />
    </declare-styleable>

    <declare-styleable name="RelativeContentContainer">
        <attr name="relative_edit_view" format="reference" />
        <attr name="relative_auto_reset_enable" format="boolean" />
        <attr name="relative_auto_reset_area" format="reference" />
    </declare-styleable>


</resources>